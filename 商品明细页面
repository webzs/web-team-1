one_count=430;

function changelocation_top(locationid)
    {
    document.formsubmit.shi.length = 1; 
    var locationid=locationid;
    var i;
    for (i=0;i < one_count; i++)
        {
            if (one_subcat[i][1] == locationid)
            { 
                document.formsubmit.shi.options[document.formsubmit.shi.length] = new Option(one_subcat[i][0], one_subcat[i][2]);
            }        
        }

    }

function changelocation_top2(locationid)
    {
    document.formsubmit2.shi.length = 1; 
    var locationid=locationid;
    var i;
    for (i=0;i < one_count; i++)
        {
            if (one_subcat[i][1] == locationid)
            { 
                document.formsubmit2.shi.options[document.formsubmit2.shi.length] = new Option(one_subcat[i][0], one_subcat[i][2]);
            }        
        }

    } 
	
function getSelected_Text(name){
var obj=document.getElementById(name);
for(i=0;i<obj.length;i++){
   if(obj[i].selected==true){
    formsubmit.sheng_hidden.value=obj[i].innerText;      //关键是通过option对象的innerText属性获取到选项文本
   }
}
//alert(getSelectedText('user_add'))
//alert(formsubmit.sheng_hidden.value)
}
function getSelected_Text1(name){
var obj=document.getElementById('sheng1');
for(i=0;i<obj.length;i++){
   if(obj[i].selected==true){
	   //alert(obj[i].innerText);
    document.getElementById('sheng_hidden1').value=obj[i].innerText;      //关键是通过option对象的innerText属性获取到选项文本
   }
}
//alert(getSelectedText('user_add'))
//alert(formsubmit.sheng_hidden.value)
}
</script>

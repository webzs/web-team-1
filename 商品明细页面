<script language="javascript" type="text/javascript">
function add(){
	if(form1.content.value==""){
		alert("请填写留言信息")
		return;
	}else{
		
			form1.action="book.asp?action=add&pid=10038&style=&music=";
			form1.submit();
		
	}
}
function edit(ed){
	if(document.getElementById("content_"+ed).value==""){
		alert("请填写回复信息")
		return;
	}else{
		if(containInvalidChartwo(document.getElementById("content_"+ed).value))
		{
			alert("对不起,你输入的回复内容中包含非法字符!");
			return;
		}else{
			form1.action="book.asp?action=edit&pid=10038&style=&music=&id="+ed;
			form1.submit();
		}
	}
}
</script>
